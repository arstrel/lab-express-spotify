<h1>Tracks </h1>

<div>
    {{#each theAlbum}}
  <h1>{{this.name}}  </h1>
  {{#if this.images.[0].url}}
  <img src="{{this.images.[0].url}}" alt="album">

  {{else}}
   <img src="http://www.designshock.com/wp-content/uploads/2016/04/man-4-400.jpg" alt="album">

  {{/if}}
  <a href="/albums/tracks/{{this.id}}">Check out the tracks</a>
  {{/each}}
</div>